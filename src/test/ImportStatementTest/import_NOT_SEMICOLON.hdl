/*
* Xor Chip
* Xor(a,b) = a And Not(b) Or b And Not(a)
*/

import And.hdl
import Or.hdl;
import Not.hdl;

CHIP Xor {
  IN a16, b16;
  OUT out16;

  PARTS:
    Not(a=a0, out=nota0);
    Not(a=b, out=notb);
    And(a=a, b=notb, out=anotb);
    And(a=b, b=nota, out=bnota);
    Or(a=anotb, b=bnota, out=out);
}