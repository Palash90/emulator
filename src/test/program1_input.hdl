/*
* Not CHIP
*/

CHIP Not {
    IN b, a[26];
    OUT                   out;

    PARTS          :
     
     Nor     (a=a, b=a, out=out); // If we short two inputs of nor gate, we essentially get a not gate.                                                                                         
}

