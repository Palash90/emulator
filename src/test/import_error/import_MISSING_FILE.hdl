/*
* Xor Chip
* Xor(a,b) = a And Not(b) Or b And Not(a)
*/

import;
import Or.hdl;
import Not.hdl;

CHIP Xor {
  IN a[16], b[16];
  OUT out[16];

  PARTS:
    Not(a=a[0], out=nota[0]);
    Not(a=b, out=notb);
    And(a=a, b=notb, out=anotb);
    And(a=b, b=nota, out=bnota);
    Or(a=anotb, b=bnota, out=out);
}